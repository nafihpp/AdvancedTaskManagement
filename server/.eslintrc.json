{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "quotes":"off",
        "linebreak-style":"off",
        "indent":["error",4],
        "spaced-comment":"off",
        "prefer-destructuring":"off"
    }
}
